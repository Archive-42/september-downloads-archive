{
    "name": "new-iscroll",
    "description": "Smooth scrolling for the web",
    "version": "18.10.8",
    "url": "http://iscrolljs.com",
    "author": "xgqfrms",
    "license": "MIT",
    "scripts": {
        "app": "npm run rmrf && webpack -p && npm run rmrf-map",
        "dev": "npm run test",
        "rmrf": "rimraf build/js/*.js",
        "rmrf-map": "rimraf build/**/*.map",
        "test": "start http://localhost:3000/demos/index.html && browser-sync start --server --files './demos/*.*'"
    },
    "bin": {
        "dev": "dev"
    },
    "main": "build/iscroll.js",
    "devDependencies": {
        "jshint": "~2.9.1",
        "uglify-js": "~2.6.2"
    },
    "keywords": [
        "scroll",
        "android",
        "mobile",
        "desktop"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/cubiq/iscroll.git"
    },
    "bugs": {
        "url": "https://github.com/cubiq/iscroll/issues"
    }
}